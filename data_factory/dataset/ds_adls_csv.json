{
	"name": "ds_adls_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ADLSLinkedService",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"filename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat('sk_',dataset().filename,'.csv') ",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('data/sharedknow/',dataset().filename) ",
					"type": "Expression"
				},
				"fileSystem": "raw"
			},
			"columnDelimiter": "|",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "\"Id\"|\"Name\"|\"UserId\"|\"Date\"",
				"type": "String"
			}
		]
	}
}